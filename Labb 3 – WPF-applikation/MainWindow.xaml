<Window x:Class="Labb_3___WPF_applikation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb_3___WPF_applikation"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DatePicker HorizontalAlignment="Left" Grid.Column="1" Grid.Row="1" Margin="245,45,0,0" VerticalAlignment="Top" Width="160"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="203,111,436,217"/>
        <Button x:Name="Booking" Grid.Column="1" Content="Boka" HorizontalAlignment="Left" Margin="23,152,0,0" Grid.Row="1" VerticalAlignment="Top" Height="40" Width="160" RenderTransformOrigin="0.791,0.537"/>
        <Button x:Name="Visa_Bokningar" Grid.Column="1" Content="Visa Bokningar" HorizontalAlignment="Left" Margin="245,152,0,0" Grid.Row="1" VerticalAlignment="Top" Height="40" Width="160"/>
        <Button x:Name="Avboka" Grid.Column="1" Content="Avboka" HorizontalAlignment="Left" Margin="23,209,0,0" Grid.Row="1" VerticalAlignment="Top" Height="40" Width="160" RenderTransformOrigin="0.491,0.823"/>
        <TextBox x:Name="Name_Input" Grid.Column="1" HorizontalAlignment="Left" Margin="23,45,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" Width="160" Height="24"/>
        <Label x:Name="Name" Grid.Column="1" Content="Namn" HorizontalAlignment="Left" Margin="23,19,0,0" Grid.Row="1" VerticalAlignment="Top" Width="125"/>
        <Label Grid.Column="1" Content="Datum" HorizontalAlignment="Left" Margin="245,19,0,0" Grid.Row="1" VerticalAlignment="Top" Width="160"/>
        <ComboBox x:Name="Time_Box" Grid.Column="1" HorizontalAlignment="Left" Margin="23,105,0,0" Grid.Row="1" VerticalAlignment="Top" Width="160"/>
        <ComboBox x:Name="Table_Box" Grid.Column="1" HorizontalAlignment="Left" Margin="245,105,0,0" Grid.Row="1" VerticalAlignment="Top" Width="160"/>
        <Label x:Name="Time" Grid.Column="1" Content="Tid" HorizontalAlignment="Left" Margin="23,79,0,0" Grid.Row="1" VerticalAlignment="Top"/>
        <Label x:Name="Table" Grid.Column="1" Content="Bordsnummer" HorizontalAlignment="Left" Margin="245,79,0,0" Grid.Row="1" VerticalAlignment="Top"/>
        <ListBox x:Name="ListBox" Grid.Row="1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,0,0,10"/>


    </Grid>
</Window>
